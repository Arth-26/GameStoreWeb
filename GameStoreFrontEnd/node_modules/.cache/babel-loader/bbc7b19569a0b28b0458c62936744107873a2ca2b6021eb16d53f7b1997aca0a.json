n'\n          },\n          body: JSON.stringify({\n            nome: this.game.nome,\n            categoria: this.game.categoria,\n            preco: this.game.preco,\n            data_de_lancamento: this.game.data_de_lancamento,\n            description: this.game.description\n          })\n        });\n        if (!response.ok) {\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        alert('Game updated!');\n        router.push('/games');\n      } catch (error) {\n        alert('Error updating game!');\n        console.error(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["router","data","originGame","id","nome","preco","data_de_lancamento","categoria","description","game","mounted","$route","params","getGame","methods","response","fetch","ok","Error","status","json","error","console","updateGame","gamecategoria","method","headers","body","JSON","stringify","alert","push"],"sources":["C:\\Users\\Arthur\\gamestore\\src\\components\\UpdateGame.vue"],"sourcesContent":["<template>\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css\">\r\n    <body class=\"home\">\r\n        <div>\r\n        \r\n        <div class=\"card\" style=\"width: 18rem;\">\r\n            <div class=\"icon\">\r\n                <i class=\"fas fa-gamepad\"></i>\r\n            </div>\r\n            <div class=\"card-body\">\r\n                <h5 class=\"card-title\">{{ originGame.nome }}</h5>\r\n                <h6 class=\"card-subtitle mb-2 text-body-secondary\">{{ originGame.categoria }}</h6>\r\n                <p class=\"card-text\">{{ originGame.description }}</p>\r\n                <div class=\"card-footer\">\r\n                    <p class=\"price\">Pre√ßo: {{ originGame.preco }}</p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <form @submit.prevent=\"updateGame\" class=\"m-5\">\r\n            <p id=\"gameregister\">Update Game</p>\r\n          