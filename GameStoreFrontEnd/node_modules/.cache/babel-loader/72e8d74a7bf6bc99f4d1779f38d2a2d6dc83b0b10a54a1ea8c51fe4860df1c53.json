{"ast":null,"code":"export default {\n  name: 'GameItem',\n  data() {\n    return {\n      result: [],\n      game: {\n        id: '',\n        nome: '',\n        preco: '',\n        data_de_lancamento: '',\n        categoria: ''\n      }\n    };\n  },\n  mounted() {\n    this.getGameList();\n  },\n  methods: {\n    async getGameList() {\n      try {\n        const response = await fetch('http://localhost:8080/games');\n        if (!response.ok) {\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const responseData = await response.json();\n        const gameList = responseData.content || [];\n        this.result = gameList;\n        this.originalList = gameList.map(p => ({\n          ...p\n        }));\n      } catch (error) {\n        alert('Error loading games!');\n        console.error(error);\n      }\n    },\n    async deleteGame(game) {\n      const confirmed = confirm('Are you sure you want to delete this product?');\n      if (confirmed) {\n        try {\n          const response = await fetch(`http://localhost:8080/games/delete/${game.Id}`, {\n            method: 'DELETE'\n          });\n          if (!response.ok) {\n            throw new Error(`HTTP error! Status: ${response.status}`);\n          }\n          alert('Game deleted successfully!');\n          this.getGameList();\n        } catch (error) {\n          alert('Error deleting game!');\n          console.error(error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","result","game","id","nome","preco","data_de_lancamento","categoria","mounted","getGameList","methods","response","fetch","ok","Error","status","responseData","json","gameList","content","originalList","map","p","error","alert","console","deleteGame","confirmed","confirm","Id","method"],"sources":["C:\\Users\\Arthur\\gamestore\\src\\components\\GameList.vue"],"sourcesContent":["<template>\r\n  <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css\">\r\n  <body class=\"home\">\r\n    <div>\r\n      <div class=\"top-bar\">\r\n        <h2>Game View </h2>\r\n        <button type=\"button\" id=\"newbutton\" class=\"btn btn-primary\"\r\n          @click=\"$router.push({ name: 'NewGame' })\">New</button>\r\n      </div>\r\n\r\n      <div class=\"l-card\">\r\n        <div v-for=\"game in result\" :key=\"game.Id\" class=\"col\">\r\n          <div class=\"card\" style=\"width: 18rem;\">\r\n            <div class=\"icon\">\r\n              <i class=\"fas fa-gamepad\"></i>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <h5 class=\"card-title\">{{ game.nome }}</h5>\r\n              <h6 class=\"card-subtitle mb-2 text-body-secondary\">{{ game.categoria }}</h6>\r\n              <p class=\"card-text\">{{ game.description }}</p>\r\n              <div class=\"box-price\">\r\n                <p class=\"price\">Pre√ßo: R$ {{ game.preco }}</p>\r\n              </div>\r\n              <button type=\"button\" id=\"edit-button\" class=\"btn btn-warning\" @click=\"$router.push({ name: 'UpdateGame', params: { id: game.Id } })\">Edit</button>\r\n              <button type=\"button\" id=\"delete-button\" class=\"btn btn-danger\" @click=\"deleteGame(game)\">Delete</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </body>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n  name: 'GameItem',\r\n  data() {\r\n    return {\r\n      result: [],\r\n\r\n      game: {\r\n\r\n        id: '',\r\n        nome: '',\r\n        preco: '',\r\n        data_de_lancamento: '',\r\n        categoria: ''\r\n\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getGameList()\r\n  },\r\n  methods: {\r\n    async getGameList() {\r\n      try {\r\n        const response = await fetch('http://localhost:8080/games');\r\n        if (!response.ok) {\r\n          throw new Error(`HTTP error! Status: ${response.status}`);\r\n        }\r\n\r\n        const responseData = await response.json();\r\n        const gameList = responseData.content || [];\r\n        this.result = gameList;\r\n        this.originalList = gameList.map(p => ({ ...p }));\r\n      } catch (error) {\r\n        alert('Error loading games!');\r\n        console.error(error);\r\n      }\r\n    },\r\n\r\n    async deleteGame(game) {\r\n      const confirmed = confirm('Are you sure you want to delete this product?')\r\n      if (confirmed) {\r\n        try {\r\n          const response = await fetch(`http://localhost:8080/games/delete/${game.Id}`, {\r\n            method: 'DELETE',\r\n          });\r\n\r\n          if (!response.ok) {\r\n            throw new Error(`HTTP error! Status: ${response.status}`);\r\n          }\r\n\r\n          alert('Game deleted successfully!');\r\n          this.getGameList();\r\n        } catch (error) {\r\n          alert('Error deleting game!');\r\n          console.error(error);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.l-card {\r\n  display: grid;\r\n  grid-gap: 30px;\r\n  grid-template-columns: repeat(2, 300px);\r\n  justify-content: center;\r\n  justify-items: center;\r\n  margin-right: 20%;\r\n}\r\n\r\n.top-bar {\r\n  display: flex;\r\n  background-color: rgba(37, 81, 134, 1);\r\n  margin-bottom: 20px;\r\n}\r\n\r\nh2 {\r\n  color: white;\r\n  text-align: center;\r\n  margin: auto;\r\n}\r\n\r\n.card-body .btn{\r\n    margin: 5px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    font-size: 16px;\r\n    transition: background-color 0.3s ease;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n</style>"],"mappings":"AAqCA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MAEVC,IAAI,EAAE;QAEJC,EAAE,EAAE,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,kBAAkB,EAAE,EAAE;QACtBC,SAAS,EAAE;MAEb;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAG;MAClB,IA